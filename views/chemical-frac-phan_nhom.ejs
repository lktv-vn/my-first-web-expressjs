</head>
<body>

    <%- include('partials/header') %>
    <div id="breadcrumb"></div>
    <section class="zzzz" >
        <% if (smallMenu[idMenu].phan_loai ==1) { %>
            <%- smallMenu[idMenu].content %>
        <% } %>
        <% if (smallMenu[idMenu].phan_loai ==2) { %>
            <%- smallMenu[idMenu].content %>
        <% } %>
    </section>
    <h3 style="text-align: center; font-size: 1.8rem;">Có thể bạn muốn xem các nhóm khác</h3>
    <section class="cc">
        <% smallMenu.forEach(element => { %>
            <a href="/home-topic/chemical/<%= element.id %>" ><%= "["+element.id_group+"] - "+ " " +element._name %></a>
        <% }) %>
    </section>
    <%- include('partials/footer') %>
    <script type="module" src="/js/breadcrumb.js"></script>  
    
    <script type="module" src="/js/script.js"></script>
